{
  "name": "coaching-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "dev:next": "next dev",
    "build": "next build",
    "build:experimental": "next build --experimental-build-mode=compile",
    "build:prod": "NODE_ENV=production next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "NODE_ENV=production node server.js",
    "start:next": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "next lint --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit --project tsconfig.strict.json",
    "type-coverage": "npx type-coverage --strict --at-least 85",
    "code-quality": "npm run type-check && npm run lint:strict && npm run format:check",
    "code-quality:strict": "npm run type-check:strict && npm run lint:strict && npm run format:check && npm run type-coverage",
    "test": "jest --project tsconfig.test.json",
    "test:watch": "jest --watch --project tsconfig.test.json",
    "test:coverage": "jest --coverage --project tsconfig.test.json",
    "test:ci": "jest --ci --coverage --watchAll=false --project tsconfig.test.json",
    "seed": "tsx prisma/seed.ts",
    "seed:sample-data": "tsx scripts/seed-sample-data.js",
    "seed:new-scorecard": "tsx scripts/seed-new-scorecard-data.js",
    "db:push": "npx prisma db push",
    "db:seed": "npm run db:push && npm run seed",
    "db:migrate": "npx prisma migrate deploy",
    "db:studio": "npx prisma studio",
    "db:setup-postgres": "./scripts/setup-database-docker.sh",
    "db:migrate-sqlite": "node scripts/migrate-sqlite-to-postgres.js",
    "db:validate": "node scripts/validate-migration.js",
    "postinstall": "prisma generate",
    "clean": "rm -rf .next node_modules package-lock.json && npm install",
    "fix:client-components": "node scripts/fix-client-components.mjs"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@hookform/resolvers": "^5.1.1",
    "@prisma/client": "^6.12.0",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@sentry/nextjs": "^8.55.0",
    "@sentry/profiling-node": "^8.42.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/crypto-js": "^4.2.2",
    "@types/exceljs": "^0.5.3",
    "@types/ioredis": "^4.28.10",
    "@types/nodemailer": "^6.4.17",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "exceljs": "^4.4.0",
    "ioredis": "^5.6.1",
    "isomorphic-dompurify": "^2.26.0",
    "lucide-react": "^0.525.0",
    "next-auth": "^5.0.0-beta.29",
    "nodemailer": "^6.10.1",
    "prisma": "^6.12.0",
    "rate-limiter-flexible": "^7.1.1",
    "react": "^18.3.1",
    "react-day-picker": "^9.8.0",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.60.0",
    "react-hot-toast": "^2.5.2",
    "recharts": "^3.1.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.0.3",
    "winston": "^3.17.0",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "glob": "^11.0.3",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jsdom": "^26.1.0",
    "msw": "^2.10.4",
    "next": "^15.4.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.3",
    "type-coverage": "^2.29.7",
    "typescript": "^5"
  }
}
